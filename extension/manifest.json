{
  "manifest_version": 2,

  "name": "AAH",
  "description": "This extension allows the user to scrape data from Foundation Center profile pages.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
  {
    "matches": [
      "https://www.linkedin.com/in/"
    ],
  }
]
}
